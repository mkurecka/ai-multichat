# Production environment variables
COMPOSE_PROJECT_NAME=ai-multichat-prod

# MySQL configuration - CHANGE THESE IN PRODUCTION!
MYSQL_DATABASE=app
MYSQL_USER=app
MYSQL_PASSWORD=!ChangeMeInProduction!
MYSQL_ROOT_PASSWORD=!ChangeMeInProduction!

# Backend URL for frontend - Set to your domain
VITE_API_URL=https://your-domain.com/api

# Security settings
APP_ENV=prod
APP_SECRET=!ChangeMeInProduction!
CORS_ALLOW_ORIGIN=^https?://(your-domain\.com|localhost|127\.0\.0\.1)(:[0-9]+)?$
FRONTEND_URL=https://your-domain.com

# JWT settings
JWT_SECRET_KEY=!ChangeMeInProduction!
JWT_PUBLIC_KEY=!ChangeMeInProduction!
JWT_PASSPHRASE=!ChangeMeInProduction!

# OpenRouter API settings
OPENROUTER_API_KEY=your-api-key
OPENROUTER_CACHE_TTL=3600 